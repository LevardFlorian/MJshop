{
    "require": {
        "johnpbloch/wordpress": "^6.2",
        "wpackagist-plugin/wordpress-seo": "^18.6",
        "wpackagist-theme/twentyseventeen": "^2.9"
    },
    "config": {
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "composer/installers": true
        }
    },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ]
}
